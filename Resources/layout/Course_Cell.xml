<?xml version="1.0" encoding="utf-8" ?> 
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/linearCourse"
    android:orientation="horizontal"
    android:layout_width="match_parent"
    android:layout_height="35.0dp" >

    <TextView
        android:id="@+id/date_cell"
        android:layout_width="80.0dp"
        android:layout_height="@dimen/height_ligne"
        android:lineHeight="0.1dp"/>
    <LinearLayout
        android:layout_width="1dp"
        android:layout_height="@dimen/height_ligne"
        android:orientation="vertical"
        android:background="@color/colorcadre"/>

    <TextView
        android:id="@+id/sens_cell"
        android:layout_width="40.0dp"
        android:layout_height="@dimen/height_ligne"/>
    <LinearLayout
        android:layout_width="1dp"
        android:layout_height="@dimen/height_ligne"
        android:orientation="vertical"
        android:background="@color/colorcadre"/>

    <TextView
        android:id="@+id/tour_cell"
        android:layout_width="40.0dp"
        android:layout_height="@dimen/height_ligne"/>
    <LinearLayout
        android:layout_width="1dp"
        android:layout_height="@dimen/height_ligne"
        android:orientation="vertical"
        android:background="@color/colorcadre"/>

    <TextView
        android:id="@+id/depart_cell"
        android:layout_width="40.0dp"
        android:layout_height="@dimen/height_ligne"/>
    <LinearLayout
        android:layout_width="1dp"
        android:layout_height="@dimen/height_ligne"
        android:orientation="vertical"
        android:background="@color/colorcadre"/>

    <TextView
        android:id="@+id/arrivee_cell"
        android:layout_width="40.0dp"
        android:layout_height="@dimen/height_ligne"/>
    <LinearLayout
        android:layout_width="1dp"
        android:layout_height="@dimen/height_ligne"
        android:orientation="vertical"
        android:background="@color/colorcadre"/>

    <TextView
        android:id="@+id/effectif_cell"
        android:layout_width="40.0dp"
        android:layout_height="@dimen/height_ligne"/>
    <LinearLayout
        android:layout_width="1dp"
        android:layout_height="@dimen/height_ligne"
        android:orientation="vertical"
        android:background="@color/colorcadre"/>

</LinearLayout>
